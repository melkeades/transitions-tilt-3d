{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["import", "sort-imports-es6-autofix"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": { "no-unused-vars": "off", "prettier/prettier": "warnings" }
}
